<div class="row">
  <div class ="col-md-6 col-md-offset-3">
    <div class="well">
      <h2>Sign Up</h2>



      <%= simple_form_for @user do |f|  %>
      <%= render partial: 'shared/errors', locals: {object: @user} %>

      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_confirmation %>

      <%= f.button :submit %>
      <% end %>





          </div>
        </div>
      </div>
